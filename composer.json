{
    "name": "phespro/phespro",
    "description": "An opinionated minimalistic and cache free open source PHP framework.",
    "type": "library",
    "repositories": [
        {
            "type": "path",
            "url": "extensions/*",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-pdo": "*",
        "phespro/container": "^1.0",
        "symfony/console": "^6.3",
        "league/route": "^5.1",
        "laminas/laminas-diactoros": "^3.1",
        "laminas/laminas-httphandlerrunner": "^2.7",
        "mschop/notee": "^1.0.1",
        "psr/log": "^3.0",
        "revolt/event-loop-adapter-react": "^1.1",
        "phespro/reactphp-server": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.3",
        "infection/infection": "^0.27"
    },
    "autoload": {
        "psr-4": {
            "Phespro\\Phespro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phespro\\Phespro\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "mschop",
            "email": "mschopdev@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true
        }
    }
}
