{
    "name": "phespro/phespro",
    "description": "An opinionated minimalistic and cache free open source PHP framework.",
    "type": "library",
    "require": {
        "php": "~8.0.0",
        "phespro/container": "dev-main",
        "symfony/console": "^5.1",
        "league/route": "^4.5",
        "laminas/laminas-diactoros": "^2.5",
        "laminas/laminas-httphandlerrunner": "^1.3",
        "mschop/notee": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Phespro\\Phespro\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phespro\\Phespro\\Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "mschop",
            "email": "mschopdev@gmail.com"
        }
